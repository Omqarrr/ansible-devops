---
apiVersion: "{{ data.apiVersion }}"
kind: "{{ data.kind }}"
metadata:
  name: "{{ data.metadata.name }}"
  labels:
{% for label in data.metadata.labels %}
    {{ label }}: {{ data.metadata.labels[label] }}
{% endfor %}
spec:
{% for item in data.spec %}
  {{ item }}: {{ data.spec[item] }}
{% endfor %}
